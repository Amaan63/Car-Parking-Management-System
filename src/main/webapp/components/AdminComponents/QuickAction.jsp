<div class="card bg-dark text-light border border-secondary mb-3">
	<div class="card-header border-secondary">
		<h5 class="card-title mb-0">Quick Actions</h5>
	</div>
	<div class="card-body">
		<div class="row g-3">
			<!-- Row 1 -->
			<div class="col-md-4">
				<div class="d-grid">
					<button class="btn btn-outline-primary p-4">
						<i class="fas fa-car-side fa-2x mb-2"></i><br /> New Booking
					</button>
				</div>
			</div>
			<div class="col-md-4">
				<div class="d-grid">
					<button class="btn btn-outline-success p-4">
						<i class="fas fa-clock fa-2x mb-2"></i><br /> Extend Time
					</button>
				</div>
			</div>
			<div class="col-md-4">
				<div class="d-grid">
					<button class="btn btn-outline-warning p-4">
						<i class="fas fa-map-marker-alt fa-2x mb-2"></i><br /> Locate
						Slot
					</button>
				</div>
			</div>

			<!-- Row 2 -->
			<div class="col-md-4">
				<div class="d-grid">
					<button class="btn btn-outline-danger p-4">
						<i class="fas fa-car-crash fa-2x mb-2"></i><br /> Cancel Booking
					</button>
				</div>
			</div>
			<div class="col-md-4">
				<div class="d-grid">
					<button class="btn btn-outline-info p-4">
						<i class="fas fa-receipt fa-2x mb-2"></i><br /> View Receipt
					</button>
				</div>
			</div>
			<div class="col-md-4">
				<div class="d-grid">
					<button class="btn btn-outline-primary p-4">
						<i class="fas fa-info-circle fa-2x mb-2"></i><br /> Help &
						Support
					</button>
				</div>
			</div>
		</div>
	</div>
</div>

